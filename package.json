{
  "name": "wysiwyg-html-editor",
  "version": "0.2.4",
  "description": "A reusable \"What You See Is What You Get\" html text editor library for PureCloud",
  "main": "lib.js",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/inindca/wysiwyg-html-editor.git"
  },
  "scripts": {
    "bundle": "webpack -p",
    "check-build": ". ./scripts/check-build",
    "doc": "jsdoc2md --plugin dmd-bitbucket --src \"src/**/*.js\" > API.md",
    "example": "echo 'Open a browser to localhost:8000/example' && python -m SimpleHTTPServer",
    "lint": "eslint .",
    "watch": "nodemon --watch src -e js,hbs,json --exec \"webpack\""
  },
  "author": "Interactive Intelligence, Inc.",
  "homepage": "https://bitbucket.org/inindca/wysiwyg-html-editor#readme",
  "publishConfig": {
    "registry": "https://purecloud.artifactoryonline.com/purecloud/api/npm/inin-internal-npm-dev/"
  },
  "files": [
    "lib.js",
    "lib.js.map"
  ],
  "engines": {
    "node": ">= 6.5.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "css-loader": "^0.23.1",
    "dmd-bitbucket": "^0.1.9",
    "eslint": "^3.4.0",
    "eventemitter3": "^1.2.0",
    "handlebars-loader": "^1.3.0",
    "imports-loader": "^0.6.5",
    "jsdoc-to-markdown": "^1.3.6",
    "json-loader": "^0.5.4",
    "lodash": "^4.13.1",
    "nodemon": "^1.11.0",
    "pre-push": "^0.1.1",
    "quill": "1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "pre-push": [
    "check-build",
    "lint"
  ]
}
